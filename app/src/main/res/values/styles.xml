        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowTranslucentStatus" tools:targetApi="19">false</item>
        <item name="android:windowDrawsSystemBarBackgrounds" tools:targetApi="21">true</item>
        <item name="android:statusBarColor" tools:targetApi="21">@color/default_DkBlue</item>
        <item name="android:navigationBarColor" tools:targetApi="21">@color/default_DkBlue</item>
        <item name="android:windowBackground">@drawable/filled_box</item> <!--Dialog transparent-->
    </style>
    
    <style name="WhiteNoTitle" parent="AppBaseTheme2">
        <item name="android:windowBackground">@color/white</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/_12sp</item>
        <item name="android:windowActionBarOverlay">true</item>
        <item name="android:windowDisablePreview">true</item>
        <!--  <item name="android:textSelectHandle">@drawable/point</item> -->
    </style>

    <style name="AppTheme.NoActionBar" parent="AppBaseTheme2">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
    </style>

    <!--New default app theme-->
    <style name="Theme.newDesign"  parent="AppBaseTheme2"></style>

